.clear {
  clear: both;
}
page {
  width: 100%;
  background: #f8f8f8;
}
.titles {
  width: 100%;
  height: 102rpx;
  padding: 0rpx 40rpx;
  line-height: 102rpx;
  background: #FFF;
  border: 1rpx solid #f2f2f2;
  border-left: none;
  border-right: none;
}
.titles .tl {
    float: left;
    color: #323232;
    font-size: 28rpx;
}
.titles .tr {
    float: right;
    color: #323232;
    font-size: 28rpx;
}
.aa {
  width: 100%;
  padding: 0rpx 40rpx;
  background: #FFF;
}
.aa .list {
    height: 100rpx;
    line-height: 100rpx;
    border-bottom: 1rpx solid #f2f2f2;
}
.aa .list .vl {
      float: left;
      color: #323232;
      font-size: 28rpx;
}
.aa .list .vr {
      float: right;
      color: #008ab0;
      font-size: 28rpx;
}

