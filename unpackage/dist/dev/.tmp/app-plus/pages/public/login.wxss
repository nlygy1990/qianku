page {
  background: #fff;
  height: 100vh;
}
.container {
  position: relative;
  width: 100vw;
  height: 100vh;
  /* overflow: hidden; */
  background: #FFFFFF;
}
.container .topbg {
    height: 490rpx;
    width: 100%;
    background-color: #0490b7;
    background: url("https://wxapp.tegou100.com/static/images/login_bg.png") center center;
    position: relative;
}
.container .topbg .toptitle {
      width: 100%;
      padding-top: var(--status-bar-height);
      text-align: center;
      color: #FFFFFF;
      font-size: 40rpx;
      position: absolute;
      z-index: 9;
      top: 34rpx;
      left: 0rpx;
}
.container .topbg .logo {
      bottom: 170rpx;
      position: absolute;
      width: 100vw;
      text-align: center;
}
.container .topbg .logo image {
        width: 110px;
        height: 35px;
}
.container .topbg .topbottom {
      position: absolute;
      left: 0;
      bottom: -1rpx;
      width: 100vw;
      height: 150rpx;
}
.container .topbg .topbottom .tleft {
        width: 50%;
        float: left;
        text-align: center;
        color: #d7f8ff;
        font-size: 30rpx;
        line-height: 150rpx;
}
.container .topbg .topbottom .active {
        color: #fFFFFF;
        font-size: 32rpx;
        font-weight: 600;
        background: url(data:image/png;base64,) no-repeat center bottom;
        background-size: 10%;
}
.wrapper {
  position: relative;
  z-index: 90;
  background: #fff;
  padding-bottom: 40rpx;
}
.back-btn {
  position: absolute;
  left: 30rpx;
  z-index: 9999;
  top: calc(var(--status-bar-height) + 40rpx);
  font-size: 40rpx;
  color: #FFFFFF;
  width: 32rpx;
  height: 32rpx;
}
.input-content {
  padding: 40rpx 30rpx;
}
.input-item {
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
      -ms-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: start;
  -webkit-align-items: flex-start;
      -ms-flex-align: start;
          align-items: flex-start;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
      -ms-flex-pack: center;
          justify-content: center;
  height: 100rpx;
  border-radius: 4px;
  margin-bottom: 10rpx;
  border-bottom: 1rpx solid #eaeaea;
}
.input-item:last-child {
    margin-bottom: 0;
}
.input-item .tit {
    height: 50rpx;
    line-height: 56rpx;
    font-size: 26rpx;
    color: #606266;
}
.input-item input {
    height: 50rpx;
    font-size: 30rpx;
    color: #303133;
    width: 100%;
}
.input-item text {
    position: absolute;
    right: 0rpx;
    font-size: 30rpx;
    color: #008ab0;
    border: 1rpx solid #008ab0;
    padding: 8rpx 30rpx;
    border-radius: 50rpx;
    z-index: 999;
}
.input-item .canvas-img-code {
    position: absolute;
    right: 120rpx;
    z-index: 999;
}
.input-item .canvas-img-code canvas {
      border: 1rpx solid #eaeaea;
      background: #eaeaea;
}
.input-item .uni-icon {
    position: absolute;
    right: 20rpx;
    font-size: 40rpx;
    z-index: 999;
    color: #008ab0;
}
.input-item .uni-icon image {
      width: 40rpx;
      height: 40rpx;
}
.input-item .kabuqing {
    position: absolute;
    right: 0;
    z-index: 999;
    color: #008ab0;
    font-size: 30rpx;
}
.confirm-btn {
  width: 690rpx;
  height: 76rpx;
  line-height: 76rpx;
  border-radius: 50px;
  margin-top: 30rpx;
  background: #008ab0;
  color: #fff;
  font-size: 32rpx;
}
.confirm-btn:after {
    border-radius: 100px;
}
.forget-section {
  font-size: 30rpx;
  color: #008ab0;
  text-align: center;
  margin-top: 40rpx;
}
.forget-section :first-child {
    float: left;
    padding-left: 30rpx;
}
.forget-section :last-child {
    float: right;
    padding-right: 30rpx;
}
.qita {
  width: 690rpx;
  margin: 0 30rpx;
  border-top: 1rpx solid #E6E6E6;
  margin-top: 110rpx;
}
.qita .tti {
  width: 250rpx;
  margin-left: 220rpx;
  font-size: 32rpx;
  background: #FFFFFF;
  margin-top: -25rpx;
  text-align: center;
}
.login-fs {
  width: 690rpx;
  text-align: center;
  margin-top: 50rpx;
}
.login-fs button {
  background: none;
  border: 1rpx solid #FFFFFF;
  float: left;
  width: 50%;
  position: relative;
  color: #FFFFFF;
}
.login-fs button:after {
  border: none;
}
.login-fs image {
  width: 70rpx;
  height: 70rpx;
  margin: 0 30rpx;
}

