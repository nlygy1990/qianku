page {
  background: #FFFFFF;
  width: 100%;
  height: 100%;
}
page .content {
    background: #FFFFFF;
}
page .content .ctop {
      background: #f6f7f7;
      font-size: 24rpx;
      color: #808080;
      padding: 30rpx 40rpx;
      line-height: 35rpx;
}
page .content .ctop text {
        color: #37b0c9;
}
page .content .gm-list {
      width: 670rpx;
      height: 144rpx;
      border-bottom: 1rpx solid #f2f2f2;
      margin: 0rpx 40rpx;
      position: relative;
}
page .content .gm-list .name {
        color: #323232;
        font-size: 28rpx;
        padding-top: 35rpx;
}
page .content .gm-list .name text {
          font-size: 28rpx;
          padding-left: 100rpx;
}
page .content .gm-list .idcard {
        color: #808080;
        font-size: 26rpx;
        padding-top: 10rpx;
}
page .content .gm-list .xuan {
        width: 38rpx;
        height: 38rpx;
        position: absolute;
        top: 53rpx;
        right: 0rpx;
}
page .content .gm-list .xuan image {
          width: 100%;
          height: 100%;
}
page .content .add {
      color: #008ab0;
      font-size: 28rpx;
      padding: 30rpx 40rpx;
}
page .content .queding {
      width: 670rpx;
      height: 80rpx;
      line-height: 80rpx;
      text-align: center;
      color: #FFFFFF;
      background: #008ab0;
      border-radius: 8rpx;
      position: fixed;
      bottom: 30rpx;
      left: 40rpx;
}

